<div class="jumbotron center">

<h2>Here are your mems, yo</h2>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mems.each do |mem| %>
      <tr>
        <td><%= mem.question %></td>
        <td><%= link_to 'Show', mem %></td>
        <td><%= link_to 'Edit', edit_mem_path(mem) %></td>
        <td><%= link_to 'Destroy', mem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mem', new_mem_path %>
